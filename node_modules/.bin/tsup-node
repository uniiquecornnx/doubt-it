#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules/tsup/dist/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules/tsup/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules/tsup/dist/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules/tsup/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/tsup@7.2.0_ts-node@10.9.1_typescript@5.2.2/node_modules:/mnt/c/Users/hp/Desktop/DOUBT-IT_NEW/aptos-ts-sdk/examples/typescript-esm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
